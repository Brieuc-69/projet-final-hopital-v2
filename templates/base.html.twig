<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Cabinet Medical!{% endblock %}</title>

    {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('asset/styles/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('asset/styles/app.css') }}">
    <link rel="stylesheet" href="{{ asset('asset/styles/maicons.css') }}">
    <link rel="stylesheet" href="{{ asset('asset/styles/animate.css') }}">
    <link rel="stylesheet" href="{{ asset('asset/styles/owl.carousel.css') }}">
    {% endblock %}
    
    <script src="{{ asset('asset/js/bootstrap.bundle.min.js') }}"></script>
    <script src="{{ asset('asset/js/google-maps.js') }}"></script>
    <script src="{{ asset('asset/js/jquery-3.5.1.min.js') }}"></script>
    <script src="{{ asset('asset/js/owl.carousel.js') }}"></script>
    <script src="{{ asset('asset/js/owl.carousel.min.js') }}"></script>
    <script src="{{ asset('asset/js/theme.js') }}"></script>

    {% block javascripts %}
    {% block importmap %}{{ importmap('app') }}{% endblock %}
    {% endblock %}
</head>
<body>
    <header>
        <div class="topbar">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8 text-sm">
                        <div class="site-info">
                            <a href="#"><span class="mai-call text-primary"></span>06 25 34 56 78</a>
                            <span class="divider">|</span>
                            <a href="#"><span class="mai-mail text-primary"></span>CabinetMédical@gmail.com</a>
                        </div>
                    </div>
                    <div class="col-sm-4 text-right text-sm">
                        <div class="social-mini-button">
                            <a href="#"><span class="mai-logo-facebook-f"></span></a>
                            <a href="#"><span class="mai-logo-twitter"></span></a>
                            <a href="#"><span class="mai-logo-dribbble"></span></a>
                            <a href="#"><span class="mai-logo-instagram"></span></a>
                        </div>
                    </div>
                </div> <!-- .row -->
            </div> <!-- .container -->
        </div> <!-- .topbar -->

        <nav class="navbar navbar-expand-lg navbar-light shadow-sm">
            <div class="container">
                <a class="navbar-brand" href="{{path('app_home')}}"><span class="text-primary">Cabinet</span> Medical</a>

                <form action="#">
                    <div class="input-group input-navbar">
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="icon-addon1"><span class="mai-search"></span></span>
                        </div>
                        <input type="text" class="form-control" placeholder="Recherchez" aria-label="Username" aria-describedby="icon-addon1">
                    </div>
                </form>

                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupport" aria-controls="navbarSupport" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupport">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="{{path('app_about_us')}}">A propos de nous</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{path('app_medecin_index')}}">Médecins</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{path('app_appointement')}}">Prendre Rendez-vous</a>
                        </li>
                        
                        {% if app.user %}
                        <li class="nav-item">
                          <a class="nav-link" href="{{path('app_profil')}}">Profil</a>
                        </li>
                        <li class="nav-item">
                          <a class="btn btn-primary ml-lg-3" href="{{path('app_logout')}}">Déconnexion</a>
                        </li>
                        {% else %}
                        <li class="nav-item">
                            <a class="btn btn-primary ml-lg-3" href="{{path('app_login')}}">Connexion / S'enregistrer</a>
                        </li>
                        {% endif %}
                    </ul>
                </div> <!-- .navbar-collapse -->
            </div> <!-- .container -->
        </nav>

        
    </header>

    {% block body %}

    {% endblock %}


    <div class="page-section banner-home bg-image" style="background-image: url({{asset('asset/img/banner-pattern.svg')}});">
      <div class="container py-5 py-lg-0">
        <div class="row align-items-center">
          <div class="col-lg-4">
            <div class="img-banner d-none d-lg-block">
              <img src={{asset('asset/img/mobile_app.png')}} alt="">
            </div>
          </div>
          <div class="col-lg-8 wow fadeInRight">
            <h1 class="font-weight-normal mb-3">Accédez facilement à toutes les fonctionnalités à l’aide de l’application </h1>
            <a href="#"><img src={{asset('asset/img/google_play.svg')}} alt=""></a>
            <a href="#" class="ml-2"><img src={{asset('asset/img/app_store.svg')}} alt=""></a>
          </div>
        </div>
      </div>
    </div> <!-- .banner-home -->
  
    <footer class="page-footer">
      <div class="container">
        <div class="row px-md-3">
          <div class="col-sm-6 col-lg-3 py-3">
            <h5>Compagnie</h5>
            <ul class="footer-menu">
              <li><a href="#">A Propos de nous</a></li>
              <li><a href="#">Carriere</a></li>
              <li><a href="#">Editeurs</a></li>
              <li><a href="#">Protection</a></li>
            </ul>
          </div>
          <div class="col-sm-6 col-lg-3 py-3">
            <h5>Plus</h5>
            <ul class="footer-menu">
              <li><a href="#">Therme et Condition</a></li>
              <li><a href="#">Confidientalité</a></li>
              <li><a href="#">Afficher</a></li>
              <li><a href="#">Joindre les médecins</a></li>
            </ul>
          </div>
          <div class="col-sm-6 col-lg-3 py-3">
            <h5>Nos Partenaire</h5>
            <ul class="footer-menu">
              <li><a href="#">Basic Feat </a></li>
              <li><a href="#">Intersport</a></li>
              <li><a href="#">Nike</a></li>
              <li><a href="#">Pumas</a></li>
            </ul>
          </div>
          <div class="col-sm-6 col-lg-3 py-3">
            <h5>Contact</h5>
            <p class="footer-link mt-2">671 Rue De la Folie, 75013 Paris</p>
            <a href="#" class="footer-link">06 25 34 56 78 </a>
            <a href="#" class="footer-link">CabinetMédical@gmail.com</a>
  
            <h5 class="mt-3">Résaux sociaux</h5>
            <div class="footer-sosmed mt-3">
              <a href="#" target="_blank"><span class="mai-logo-facebook-f"></span></a>
              <a href="#" target="_blank"><span class="mai-logo-twitter"></span></a>
              <a href="#" target="_blank"><span class="mai-logo-google-plus-g"></span></a>
              <a href="#" target="_blank"><span class="mai-logo-instagram"></span></a>
              <a href="#" target="_blank"><span class="mai-logo-linkedin"></span></a>
            </div>
          </div>
        </div>
  
        <hr>
  
        <p id="copyright">Copyright &copy; 2024 <a href="" target="_blank">Brieuc Isoard</a>. All right reserved</p>
      </div>
    </footer>

   
</body>
</html>
